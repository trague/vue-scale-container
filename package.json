{
  "name": "vue-scale-container",
  "version": "0.0.15",
  "private": false,
  "author": "trague",
  "license": "MIT",
  "description": "用于大屏缩放的Vue组件",
  "repository": {
    "type": "git",
    "url": "https://github.com/trague/vue-scale-container.git"
  },
  "keywords": [
    "vue",
    "vue-scale-container",
    "datav"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/vue-scale-conainer.umd.js",
  "module": "lib/vue-scale-conainer.es.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vue": "^3.0.0",
    "vue-tsc": "^1.6.4",
    "@vue/composition-api": "^1.7.1"
  }
}